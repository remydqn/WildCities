<div  tabindex="1" role="dialog" aria-labelledby="mySmallModalLabel">
 <div  role="document">
   <div class="modal-content">

    <form method="post" action=<%= "#{event_votes_path}" %>>
      <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
     <div class="star-rating">
       <fieldset>
         <input type="radio" id="star5" name="vote[note]" value="5" /><label for="star5" title="Outstanding"><i class="far fa-star"></i></label>
         <input type="radio" id="star4" name="vote[note]" value="4" /><label for="star4" title="Very Good"><i class="far fa-star"></i></label>
         <input type="radio" id="star3" name="vote[note]" value="3" /><label for="star3" title="Good"><i class="far fa-star"></i></label>
         <input type="radio" id="star2" name="vote[note]" value="2" /><label for="star2" title="Poor"><i class="far fa-star"></i></label>
         <input type="radio" id="star1" name="vote[note]" value="1" /><label for="star1" title="Very Poor"><i class="far fa-star"></i></label>
       </fieldset>
        <input type="submit" value="vote" class="btn-wildcity">
     </div>
    </form>

     <%=  link_to "ta gueule", edit_criterias_path(current_user.criteria) %>

   </div>
 </div>
</div>
